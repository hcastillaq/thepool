{
	"name": "The_Pool",
	"version": "0.0.1",
	"description": "Simple boilerplate para weback y webpack-dev-server",
	"main": "server.js",
	"scripts": {
		"client:watch": " cross-env NODE_ENV=development webpack --mode development --watch --config ./webpack.config.client.js",
		"client:build": "cross-env NODE_ENV=production webpack --mode production --config ./webpack.config.client.js",
		"client:build:watch": "cross-env NODE_ENV=production webpack -w --mode production --config ./webpack.config.client.js",
		"client:server": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.client.js",
		"client:dev": "npm-run-all --parallel client:watch client:server",
		
		"server:watch": "cross-env NODE_ENV=development webpack -w --mode development --config ./webpack.config.server.js",
		"server:build": "cross-env NODE_ENV=production webpack --mode production --config ./webpack.config.server.js",
		"server:build:watch": "cross-env NODE_ENV=production webpack -w --mode production --config ./webpack.config.server.js",
		"server:server": "nodemon ./build/server.js"
	},
	"keywords": [
		"boilerplate",
		"webpack",
		"webpack-dev-server",
		"webpack configuration"
	],
	"author": "Hernan Castilla",
	"license": "ISC",
	"dependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
